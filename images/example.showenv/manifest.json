{
  "name": "example/showenv",
  "labels": [
    { "name": "version", "val": "1.1.1" }
  ],
  "app": {
    "exec": [
      "/usr/local/bin/showenv",
      "foo",
      "bar",
      "baz",
      "quux"
    ],
    "user": "games",
    "group": "guest",
    "mountPoints": [
      {"name": "data", "path": "/opt/data"}
    ],
    "environment": {
      "FOO": "bar",
      "BAZ": "quux"
    }
  }
}
